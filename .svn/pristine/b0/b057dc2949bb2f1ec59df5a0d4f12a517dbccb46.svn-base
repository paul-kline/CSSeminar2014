    void setProgressTo(int newVal) {
        jProgressBar1.setValue(newVal);
        jLabel4.setText(ByteFigTestering.getGamesCompleteSoFar() + " / " + ByteFigTestering.numberOfGamesToPlay);
    }
    void setCompleted(){
        jLabel1.setText("Completed!");
        try {
            Thread.sleep(5000);
        } catch (InterruptedException ex) {
            Logger.getLogger(Progress.class.getName()).log(Level.SEVERE, null, ex);
        }
        System.exit(1);
    }
    
