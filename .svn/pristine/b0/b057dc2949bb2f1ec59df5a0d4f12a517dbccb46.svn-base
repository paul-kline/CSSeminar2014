    public void displayUsingNN(boolean bool){
        jLabel3.setText("Use NN:  "+ bool);
    }
    public void displayWeights(int[] weights){
        String ws="";
        for (int i = 0; i < weights.length; i++) {
            ws+=weights[i] + ", ";
            
        }
        jLabel2.setText("Weights: "+ ws);
    }
